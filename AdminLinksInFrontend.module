            $field->attr('maxlength', 7);
            $field->attr('length', 7);
            $field->columnWidth = '_color' == $k ? 50 : 25;
            $field->required = true;
            $fieldset->add($field);
        }
        $fieldset->add($Textfield);
        return $fieldset;
    }

